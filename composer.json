{
    "name": "dehbka/wigle-client",
    "description": "Client for wigle.net api",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Dehbka\\WigleClient\\": "src/"
        }
    },
    "authors": [
        {
            "name": "dehbka",
            "email": "dehbkaa@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^8.1",
        "psr/http-client": "^1.0",
        "php-http/discovery": "^1.15",
        "psr/http-factory": "^1.0",
        "guzzlehttp/guzzle": "^7.5",
        "alexanderpas/http-enum": "^1.0",
        "json-mapper/json-mapper": "^2.16"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "require-dev": {
        "nyholm/psr7": "^1.7"
    }
}
